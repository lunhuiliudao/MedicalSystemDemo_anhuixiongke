    <ReportTitle xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
      <ReportName>手术量统计</ReportName>
      <Menu>日常统计</Menu>
      <StrSql>with datasource as
(
  select t.tt, sum(t.count) as count, sum(t.zqjl) as zqjl, sum(t.zxbssl) as zxbssl, sum(t.zxqjl) as zxqjl from
  (
  select to_char(m.scheduled_date_time, 'yyyy-MM') as tt, count(*) as count, 0 as zqjl, 0 as zxbssl, 0 as zxqjl from med_operation_master m
  where m.oper_status_code &gt;= 35
   and m.scheduled_date_time &gt;= :STARTTIME
   and m.scheduled_date_time &lt;= :ENDTIME
  group by to_char(m.scheduled_date_time, 'yyyy-MM')
   
  union all
   
  select to_char(m.scheduled_date_time, 'yyyy-MM') as tt, 0 as count, count(*) as zqjl, 0 as zxbssl, 0 as zxqjl from med_operation_master m
  where m.oper_status_code &gt;= 35
  and m.scheduled_date_time &gt;= :STARTTIME
   and m.scheduled_date_time &lt;= :ENDTIME
      and m.operation_name like '%胸腔镜%' or m.operation_name like '%腹腔镜%'
  group by to_char(m.scheduled_date_time, 'yyyy-MM')
   
  union all

  select to_char(m.scheduled_date_time, 'yyyy-MM') as tt, 0 as count, 0 as zqjl, count(*) as zxbssl, 0 as zxqjl from med_operation_master m
  where m.oper_status_code &gt;= 35
  and m.scheduled_date_time &gt;= :STARTTIME
   and m.scheduled_date_time &lt;= :ENDTIME
         AND M.OPERATION_NAME like '%肺叶切除术%' or M.OPERATION_NAME like '%肺楔形术%' or M.OPERATION_NAME like '%肺病灶切除术%' or  M.OPERATION_NAME like '%胸腺切除术%' 
     or M.OPERATION_NAME like '%纵膈肿物切除术%'  or M.OPERATION_NAME like '%肺大泡修补切除术%'  or M.OPERATION_NAME like '%肺段切除术%' 
     or M.OPERATION_NAME like '%肺袖式切除术%' or M.OPERATION_NAME like '%全肺切除术%' or M.OPERATION_NAME like '%纵膈淋巴结活检术%' 
     or M.OPERATION_NAME like '%胸膜活检术%'     or M.OPERATION_NAME like '%食管癌根治术%'    or M.OPERATION_NAME like '%贲门癌根治术%'  
      or M.OPERATION_NAME like '%脓胸清除加纤维板剥脱术%'    or M.OPERATION_NAME like '%胸壁肿物切除术%'  
      or M.OPERATION_NAME like '%双侧交感神经链切除术%' 
          or M.OPERATION_NAME like '%胸骨劈开胸腺切除术%' or M.OPERATION_NAME like '%开胸探查术%'  or M.OPERATION_NAME like '%开胸止血术%'  group by to_char(m.scheduled_date_time, 'yyyy-MM')
     
  union all

  select to_char(m.scheduled_date_time, 'yyyy-MM') as tt, 0 as count, 0 as zqjl, 0 as zxbssl, count(*) as zxqjl from med_operation_master m
  where m.oper_status_code &gt;= 35
  and m.scheduled_date_time &gt;= :STARTTIME
   and m.scheduled_date_time &lt;= :ENDTIME
      and m.operation_name like '%胸腔镜%'
  group by to_char(m.scheduled_date_time, 'yyyy-MM')
  ) t
  group by t.tt
  order by t.tt
)

select * from datasource

union all

select '总计' as tt, sum(s.count), sum(s.zqjl), sum(s.zxbssl), sum(s.zxqjl)
from datasource s</StrSql>
